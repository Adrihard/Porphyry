element topic {
	attribute href {xsd:anyURI}?,
	attribute name {text}?,
	element viewpoint {
		attribute href {xsd:anyURI},
		text
	}?,
	(
		element entity {
			attribute href {xsd:anyURI},
			text
		}
	|
		element relatedTopic {
			attribute href {xsd:anyURI},
			attribute relationType {text}?,
			text
		}
	)*
}
